{
  "name": "cloudgenix-api-client",
  "version": "0.0.1",
  "description": "CloudGenix API Client with Node.js Promise Support.",
  "main": "index.js",
  "scripts": {
    "build": "babel --presets 'flow' src/ -d lib/",
    "flow": "flow",
    "pretest": "npm run flow",
    "test": "mocha",
    "up": "npm-check -u -E && rm -rf ./node_modules package-lock.json && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumitgoelpw/cloudgenix-api-client.git"
  },
  "keywords": [
    "CloudGenix",
    "API"
  ],
  "author": "Sumit Goel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sumitgoelpw/cloudgenix-api-client/issues"
  },
  "homepage": "https://github.com/sumitgoelpw/cloudgenix-api-client#readme",
  "dependencies": {
    "debug": "3.0.0",
    "moment": "2.18.1",
    "request": "2.81.0",
    "request-promise-native": "1.0.4"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-preset-flow": "6.23.0",
    "chai": "4.1.1",
    "chai-as-promised": "7.1.1",
    "coveralls": "2.13.1",
    "eslint": "4.5.0",
    "eslint-config-sumitgoelpw": "1.0.6",
    "eslint-plugin-flowtype": "2.35.0",
    "flow-bin": "0.53.1",
    "istanbul": "0.4.5",
    "mocha": "3.5.0",
    "mocha-lcov-reporter": "1.3.0",
    "npm-check": "5.4.5",
    "nsp": "2.7.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:flowtype/recommended",
      "sumitgoelpw"
    ],
    "plugins": [
      "flowtype"
    ]
  }
}
